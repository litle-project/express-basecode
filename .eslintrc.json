{
    "extends": [
      "airbnb-base"
    ],
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-async-promise-executor": 0,
      "no-constructor-return": 0,
      "semi": ["error", "always"],
      "quotes": ["error", "single"]
    }
}
